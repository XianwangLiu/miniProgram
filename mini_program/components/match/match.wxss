.team-list,
.schedule{
  display: flex;
  width:680rpx;
  height:160rpx;
  margin-left: 35rpx;
  line-height: 160rpx;
  flex-direction: row;
  background-color: whitesmoke;
  border-radius: 20rpx;
  margin-top: 10rpx;
  margin-bottom: 40rpx;
  box-shadow: 0px 10px 30px rgba(0, 0, 0, 0.2);
}

.pic{
  position: relative;
  width: 100rpx;
  height: 100rpx;
  left: 50rpx;
  top: 30rpx;
}

.title{
  position: relative;
  width: 300rpx;
  height: 90rpx;
  left: 80rpx;
  text-align: center;
  font-size: 32rpx;
}